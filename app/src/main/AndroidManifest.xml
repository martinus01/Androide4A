<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.esiea.androide4a">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".injection.EsieaApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Androide4A">
        <activity
            android:name=".presentation.main.SpaceScreenActivity"
            android:theme="@style/Theme.spacescreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.main.API.infoLaunch"></activity>
        <activity android:name=".presentation.main.LaunchActivity"></activity>
        <activity android:name=".presentation.main.RegisterUser"></activity>
        <activity android:name=".presentation.main.MainActivity"></activity>

    </application>

</manifest>